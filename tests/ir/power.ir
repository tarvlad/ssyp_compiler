#F_BEGIN;
    #F_NAME power;
    #F_ARGS_BEGIN;
        @Int x;
        @Int y;
    #F_ARGS_END;
    #F_VARS_BEGIN;
        @Int t1;
        @Int t2;
    #F_VARS_END;
    #F_BODY_BEGIN;
        #IF = y #0;
            #F_RETURN #1;
        #ELSE;
            #IF = y #1;
                #F_RETURN x;
            #ELSE;
                #- y y #1;
                #power t1 x y;
                #* x x t1;
                #F_RETURN x;
            #ENDIF;
        #ENDIF;
    #F_BODY_END;
#F_END;

#F_BEGIN;
    #F_NAME main;
    #F_ARGS_BEGIN;
    #F_ARGS_END;
    #F_VARS_BEGIN;
        @Int x;
    #F_VARS_END;
    #F_BODY_BEGIN;
        #power x #3 #2;
        #F_RETURN x;
    #F_BODY_END;
#F_END;